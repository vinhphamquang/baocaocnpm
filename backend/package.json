{
  "name": "vinfast-backend",
  "version": "1.0.0",
  "description": "Backend API cho website cửa hàng ô tô VinFast",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seedData.js",
    "export-swagger": "node scripts/exportSwagger.js",
    "generate-postman": "node scripts/generatePostman.js",
    "docs": "npm run export-swagger && npm run generate-postman && echo 'Documentation exported! Swagger: http://localhost:8000/api-docs'",
    "test-api": "echo 'Use Postman collection in docs/ folder to test API endpoints'"
  },
  "keywords": [
    "vinfast",
    "api",
    "mongodb",
    "express"
  ],
  "author": "VinFast Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
